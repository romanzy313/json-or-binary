{
  "name": "json-or-binary",
  "version": "0.9.9",
  "description": "Simply serialize discriminated unions into a binary format.",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "keywords": [
    "binary",
    "json",
    "serialize",
    "encode",
    "discriminated-union",
    "uint8array",
    "codec"
  ],
  "author": "Roman Volovoy",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/romanzy313/json-or-binary.git"
  },
  "scripts": {
    "dev": "vitest",
    "test": "vitest --run --coverage",
    "build": "rm -rf ./dist && tsc --project tsconfig.build.json",
    "prepare": "pnpm build"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "4.0.13",
    "typescript": "^5.9.3",
    "vitest": "^4.0.13"
  },
  "packageManager": "pnpm@10.23.0"
}
